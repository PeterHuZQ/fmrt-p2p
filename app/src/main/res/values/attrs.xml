<?xml version="1.0" encoding="utf-8"?>
<!--设置自定义控件的自定义属性-->
<resources>
    <declare-styleable name="RoundProgress">
        <!-- 外层圆的颜色-->
        <attr name="roundColor" format="color"/>
        <!--弧形进度圈的颜色-->
        <attr name="roundProgressColor" format="color"/>
        <!--圆环的宽度-->
        <attr name="roundWidth" format="dimension"></attr>
        <!--中间百分比文字的颜色-->
        <attr name="textColor" format="color"/>
        <!--中间百分比文字的大小-->
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="IndicatorLockView">
        <attr name="indicator_selected" format="reference" />
        <attr name="indicator_unselected" format="reference" />
    </declare-styleable>

    <!--自定义手势密码，相关属性-->
    <declare-styleable name="ExpandLockView">
        <attr name="lock_selected" format="reference"></attr>
        <attr name="lock_selected_error" format="reference"></attr>
        <attr name="lock_unselected" format="reference"></attr>
        <attr name="title_set_password_again" format="string"></attr>
        <attr name="title_set_password_error" format="string"></attr>
        <attr name="title_unlock_password_right" format="string"></attr>
        <attr name="title_unlock_password_error" format="string"></attr>
        <attr name="title_unlock_password_overtime" format="string"></attr>
        <attr name="title_reset_password_new" format="string"></attr>
        <attr name="title_reset_password_again" format="string"></attr>
        <attr name="title_reset_password_error" format="string"></attr>
    </declare-styleable>
</resources>